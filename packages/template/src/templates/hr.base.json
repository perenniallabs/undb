            "option": {
              "options": [
                {
                  "id": "vacation",
                  "name": "Vacation",
                  "color": "green"
                },
                {
                  "id": "sick",
                  "name": "Sick Leave",
                  "color": "red"
                },
                {
                  "id": "personal",
                  "name": "Personal",
                  "color": "blue"
                }
              ]
            }
          },
          "Status": {
            "id": "status",
            "type": "select",
            "defaultValue": "pending",
            "constraint": {
              "max": 1
            },
            "option": {
              "options": [
                {
                  "id": "pending",
                  "name": "Pending",
                  "color": "orange"
                },
                {
                  "id": "approved",
                  "name": "Approved",
                  "color": "green"
                },
                {
                  "id": "rejected",
                  "name": "Rejected",
                  "color": "red"
                }
              ]
            }
          },
          "Reason": {
            "id": "reason",
            "type": "longText"
          },
          "Employee Name": {
            "id": "employee_name",
            "type": "rollup",
            "option": {
              "referenceFieldId": "employee",
              "rollupFieldId": "name",
              "fn": "lookup"
            }
          }
        },
        "views": {
          "All Requests": {
            "type": "grid"
          },
          "Pending Requests": {
            "type": "grid",
            "filter": {
              "conjunction": "and",
              "children": [
                {
                  "field": "status",
                  "op": "eq",
                  "value": "pending"
                }
              ]
            }
          }
        },
        "forms": {
          "Time Off Request Form": {
            "fields": [
              {
                "fieldId": "employee",
                "hidden": false,
                "required": true
              },
              {
                "fieldId": "start_date",
